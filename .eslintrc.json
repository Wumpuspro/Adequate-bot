{
	"env": {
		"browser": false,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"space-in-parens": ["error", "always"],
		"indent": ["error", "tab"],
		"import/no-cycle": "off",
		"import/prefer-default-export": "off",
		"template-curly-spacing": "off",
		"linebreak-style": "off",
		"consistent-return": "off",
		"comma-dangle": "off",
		"no-param-reassign": "off",
		"no-underscore-dangle": "off",
		"no-plusplus": "off",
		"no-unused-vars":"off",
		"no-restricted-syntax":"off",
		"no-await-in-loop":"off",
		"no-shadow": "off",
		"no-tabs": 0,
		"no-console": "warn"
	}
}
